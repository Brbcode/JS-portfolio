{
  "name": "js-portfolio",
  "version": "1.0.0",
  "description": "Esta aplicación proporciona un buscador de libros digitales, muestra la información relacionada de dicho libro y permite descargar dicho libro.",
  "main": "index.js",
  "scripts": {
    "start:darwin:linux": "NODE_ENV=development webpack serve --mode development",
    "start:win32": "set NODE_ENV=development && webpack serve --mode development",
    "start:default": "echo 'Platfom not found'.",
    "start": "run-script-os",
    "build:darwin:linux": "NODE_ENV=production webpack --mode=production",
    "build:win32": "set NODE_ENV=production && webpack --mode=production",
    "build:default":"echo 'Platfom not found'.",
    "build":"run-script-os",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brbcode/JS-portfolio.git"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Bruno García Trípoli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Brbcode/JS-portfolio/issues"
  },
  "homepage": "https://github.com/Brbcode/JS-portfolio#readme",
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.1",
    "prettier": "^2.8.0",
    "run-script-os": "^1.1.6",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "prettier": {
    "semi": true
  }
}
